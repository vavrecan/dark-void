{
  "name": "Dark Void",
  "dark": true,
  "author": "Marek Vavrecan",
  "editorScheme": "/Dark_Void.icls",
  "ui": {
    "*": {
      "background": "#1b1b1b",
      "disabledBackground": "#1b1b1b",
      "inactiveBackground": "#1b1b1c",

      "selectionBackground": "#151515",
      "selectionInactiveBackground": "#151516",

      "lightSelectionBackground": "#151514",
      "lightSelectionInactiveBackground": "#151513",

      "borderColor": "#292929",
      "disabledBorderColor": "#333336",

      "separatorForeground": "#292929",
      "separatorColor": "#292929",
      "lineSeparatorColor": "#292929"
    },

    "ScrollBar": {
      "Transparent": {
        "trackColor": "#E6E6E600",
        "thumbColor": "#1B1B1BBA",
        "thumbBorderColor": "#161616",
        "hoverTrackColor": "#E6E6E65A",
        "hoverThumbColor": "#1B1B1BBA",
        "hoverThumbBorderColor": "#161616"
      },
      "trackColor": "#1B1B1BBA",
      "thumbColor": "#1B1B1BBA",
      "thumbBorderColor": "#161616",
      "hoverTrackColor": "#1B1B1BBA",
      "hoverThumbColor": "#1B1B1BBA",
      "hoverThumbBorderColor": "#161616",
      "background": "#1B1B1BBA"
    },

    "Borders": {
      "color": "#292929",
      "ContrastBorderColor": "#292929"
    },

    "Tree.rowHeight": 24,

    "VersionControl": {
      "Log.Commit": {
        "currentBranchBackground": "#2E3D5A",
        "unmatchedForeground": "#727380"
      },
      "FileHistory.Commit.selectedBranchBackground": "#2E3D5A"
    },

    "DefaultTabs": {
      "hoverBackground": "#00000000",
      "underlineColor": "#607d8b",
      "underlinedTabBackground": "#323232"
    },

    "SearchEverywhere": {
      "Tab": {
        "selectedForeground": "#CFD2D5",
        "selectedBackground": "#151514"
      }
    },

    "EditorTabs": {
      "selectedBackground": "#37383b",
      "inactiveMaskColor": "#00000033",
      "underlineColor": "#607d8b",
      "underlinedTabBackground": "#323232"
    },

    "ToolWindow.HeaderTab": {
      "underlinedTabBackground": "#151514"
    }
  }
}
